fun main() {
    var countD = 0
    var countC = 0
    var countB = 0
    var countA = 0
    repeat(readLine()!!.toInt()) {
        val x = readLine()!!.toInt()
        if (x == 2) countD++
        if (x == 3) countC++
        if (x == 4) countB++
        if (x == 5) countA++
    }
    println("$countD $countC $countB $countA")
}
