import java.util.Scanner
    fun main() {
        val scan = Scanner(System.`in`)
        val x1 = scan.nextInt()
        val y1 = scan.nextInt()
        val x2 = scan.nextInt()
        val y2 = scan.nextInt()
        if ((x1 == x2 || y1 == y2) || Math.abs(x1-x2) == Math.abs(y1-y2)){
            System.out.println("YES")
        }
        else{
            System.out.println("NO")
        }
    }
